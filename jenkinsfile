
node{

        stage("Checkout") {
           
                git branch: 'Master', url: 'https://github.com/zbat-omar/angular.git'
           
        }

       stage("Install node modules ") {
        bat 'npm install' } 
        stage("Build") 
        { bat 'npm run build --prod' }
        stage("Build") 
        { bat 'ng serve' }
        }
